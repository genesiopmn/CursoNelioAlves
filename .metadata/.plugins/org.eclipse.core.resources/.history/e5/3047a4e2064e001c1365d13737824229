package entities;

public class Account {

	private int accountNumber;
	private String holderName;
	private double balance;
	private double deposit;
	private double withdraw;
	private double initialBalance;
	
	public Account(int accountNumber, String holderName, double balance, double deposit, double withdraw,
			double initialBalance) {
		super();
		this.accountNumber = accountNumber;
		this.holderName = holderName;
		this.balance = balance;
		this.deposit = deposit;
		this.withdraw = withdraw;
		this.initialBalance = initialBalance;
	}

	public int getAccountNumber() {
		return accountNumber;
	}

	public void setAccountNumber(int accountNumber) {
		this.accountNumber = accountNumber;
	}

	public String getHolderName() {
		return holderName;
	}

	public void setHolderName(String holderName) {
		this.holderName = holderName;
	}

	public double getBalance() {
		return balance;
	}

//	public void setBalance(double balance) {
//		this.balance = balance;
//	}

	public double getDeposit() {
		return deposit;
	}

	public void setDeposit(double deposit) {
		this.deposit = deposit;
	}

	public double getWithdraw() {
		return withdraw;
	}

	public void setWithdraw(double withdraw) {
		this.withdraw = withdraw;
	}

	public double getInitialBalance() {
		return initialBalance;
	}

	public void setInitialBalance(double initialBalance) {
		this.initialBalance = initialBalance;
	}

	@Override
	public String toString() {
		return "Account [accountNumber=" + accountNumber + ", holderName=" + holderName + ", balance=" + balance
				+ ", deposit=" + deposit + ", withdraw=" + withdraw + ", initialBalance=" + initialBalance
				+ ", getAccountNumber()=" + getAccountNumber() + ", getHolderName()=" + getHolderName()
				+ ", getBalance()=" + getBalance() + ", getDeposit()=" + getDeposit() + ", getWithdraw()="
				+ getWithdraw() + ", getInitialBalance()=" + getInitialBalance() + "]";
	}
	
	
	
	
//	public void setDeposit(double deposit) {
//		double deposit;
//		this.balance = deposit + balance;
//		setBalance(this.deposit += this.balance);
//	}
//	public void setWithdraw(double withdraw, double balance) {
//		double withdraw = setWithdraw();
//		this.balance = this.withdraw -= this.balance;
//	}	
	
		
}
